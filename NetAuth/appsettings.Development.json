{
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=5434;Database=NetAuth;Username=postgres;Password=postgres;Include Error Detail=true",
    "Redis": "localhost:6379"
  },
  "Jwt": {
    "SecretKey": "_auth_aspnet_dot_core_hoc081098_",
    "Issuer": "hoc081098",
    "Audience": "MyAppClients",
    "Expiration": "01:00:00",
    "RefreshTokenExpiration": "02:00:00"
  },
  "Outbox": {
    "Interval": "00:00:10",
    "BatchSize": 500,
    "MaxAttempts": 3,
    "CleanupRetention": "00:00:30",
    "CleanupBatchSize": 5000
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/logs/log-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "NetAuth"
    }
  }
}
